<?xml version="1.0"?>

<PropertyList include="737-MAX-common.xml">

    <sim>
        <description>Boeing 737 MAX 8</description>
        <liveries-path type="string">Aircraft/737-MAX/Models/Liveries-8</liveries-path>
        <aero>737-8</aero>
        <variant-of>737-7</variant-of>
        <startup>
            <splash-texture>Aircraft/737-MAX/Splash/Splash-8/splash-1.png</splash-texture>
            <splash-texture>Aircraft/737-MAX/Splash/Splash-8/splash-2.png</splash-texture>
            <splash-texture>Aircraft/737-MAX/Splash/Splash-8/splash-3.png</splash-texture>
        </startup>

        <previews>
            <preview>
                <type>exterior</type>
                <splash type="bool">true</splash>
                <path>Splash/Splash-8/splash-1.png</path>
            </preview>
            <preview>
                <type>exterior</type>
                <splash type="bool">true</splash>
                <path>Splash/Splash-8/splash-2.png</path>
            </preview>
            <preview>
                <type>exterior</type>
                <splash type="bool">true</splash>
                <path>Splash/Splash-8/splash-3.png</path>
            </preview>
        </previews>

        <model>
            <door>
                <position-x-m type="float">-15.9187</position-x-m>
                <position-y-m type="float">-1.71334</position-y-m>
                <position-z-m type="float">0.00098</position-z-m>
                <jetway-hood-deg type="float">0</jetway-hood-deg>
            </door>
            <path>Aircraft/737-MAX/Models/737-8.xml</path>
            <livery>
                <file type="string">737-8</file>
            </livery>

            <rendering>
                <rain-enabled type="int">2</rain-enabled>
                <reflection-enabled type="int">1</reflection-enabled>
                <reflection-type type="int">1</reflection-type>
                <reflection-dynamic type="int">1</reflection-dynamic>
                <reflection-correction type="float">-0.8</reflection-correction>
                <reflection-fresnel type="float">0.0</reflection-fresnel>
                <reflection-fresnel-factor type="float">0.7</reflection-fresnel-factor>
                <reflection-rainbow type="float">0.0</reflection-rainbow>
                <reflection-noise type="float">0.00</reflection-noise>
                <ambient-correction type="float">0.15</ambient-correction>
            </rendering>
        </model>

        <view>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">65</default-field-of-view-deg>
                <pitch-offset-deg>-16</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <x-offset-m archive="y">-0.51</x-offset-m>
                <y-offset-m archive="y">1.1</y-offset-m>
                <z-offset-m archive="y">-17.98</z-offset-m>
            </config>
        </view>

        <view n="106">
            <name>Copilot</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">65</default-field-of-view-deg>
                <pitch-offset-deg>-16</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <x-offset-m archive="y">0.51</x-offset-m>
                <y-offset-m archive="y">1.1</y-offset-m>
                <z-offset-m archive="y">-17.98</z-offset-m>
            </config>
        </view>

        <view n="100">
            <name>CDU View</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">41</default-field-of-view-deg>
                <pitch-offset-deg>-70</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <x-offset-m archive="y">-0.15</x-offset-m>
                <y-offset-m archive="y">1.3</y-offset-m>
                <z-offset-m archive="y">-17.6</z-offset-m>
            </config>
        </view>
        <view n="101"></view>
        <view n="102"></view>
        <view n="103">
            <name>Right Wing</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">80</default-field-of-view-deg>
                <pitch-offset-deg>0</pitch-offset-deg>
                <heading-offset-deg>-90</heading-offset-deg>
                <x-offset-m archive="y">2</x-offset-m>
                <y-offset-m archive="y">1.423</y-offset-m>
                <z-offset-m archive="y">2.076</z-offset-m>
            </config>
        </view>
        <view n="104">
            <name>Left Wing</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">80</default-field-of-view-deg>
                <pitch-offset-deg>0</pitch-offset-deg>
                <heading-offset-deg>90</heading-offset-deg>
                <x-offset-m archive="y">-2</x-offset-m>
                <y-offset-m archive="y">1.423</y-offset-m>
                <z-offset-m archive="y">2.076</z-offset-m>
            </config>
        </view>
        <view n="105">
            <name>Overhead Panel</name>
            <type>lookfrom</type>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <default-field-of-view-deg type="double">108</default-field-of-view-deg>
                <pitch-offset-deg>50</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <x-offset-m archive="y">0</x-offset-m>
                <y-offset-m archive="y">1.26</y-offset-m>
                <z-offset-m archive="y">-17.26</z-offset-m>
            </config>
        </view>
    </sim>
    <instrumentation>
        <cdu>
            <display>IDENT</display>
            <ident>
                <model>737 MAX 8</model>
                <engines>CFM LEAP 1B</engines>
            </ident>
            <input type="string" />
            <serviceable type="bool">true</serviceable>
        </cdu>
    </instrumentation>

    <limits>
        <!-- According to http://wwww.b737.org.uk -->
        <mass-and-balance>
            <maximum-ramp-mass-lbs>156500</maximum-ramp-mass-lbs>
            <maximum-takeoff-mass-lbs>181200</maximum-takeoff-mass-lbs>
            <maximum-landing-mass-lbs>152800</maximum-landing-mass-lbs>
            <maximum-zero-fuel-mass-lbs>145400</maximum-zero-fuel-mass-lbs>
        </mass-and-balance>
        <max-flap-extension-speed>
            <flaps>0.125</flaps>
            <speed>250</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.250</flaps>
            <speed>250</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.375</flaps>
            <speed>250</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.500</flaps>
            <speed>210</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.625</flaps>
            <speed>200</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.750</flaps>
            <speed>190</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>0.875</flaps>
            <speed>175</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed>
            <flaps>1.000</flaps>
            <speed>158</speed>
        </max-flap-extension-speed>
        <max-gear-extension-speed>270</max-gear-extension-speed>
        <max-gear-retraction-speed>235</max-gear-retraction-speed>
        <vne>340</vne>
        <max-positive-g>2.5</max-positive-g>
    </limits>

    <payload>

        <pax>
            <business type="double">20</business>
            <tourist type="double">33</tourist>
            <economy type="double">126</economy>
        </pax>
        <weight>
            <name type="string">Business class (20 pax max)</name>
            <weight-lb alias="/payload/pax/business" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">3560</max-lb>
        </weight>
        <weight>
            <name type="string">Tourist class (33 pax max)</name>
            <weight-lb alias="/payload/pax/tourist" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">5874</max-lb>
        </weight>
        <weight>
            <name type="string">Economy class (126 pax max)</name>
            <weight-lb alias="/payload/pax/economy" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">2248</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 1A</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">1218</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 1B</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[4]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">741</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 2</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[5]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">5887</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 3</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[6]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">9008</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 4A</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[7]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">798</max-lb>
        </weight>
        <weight>
            <name type="string">Cargo compartment 4B</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[8]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">886</max-lb>
        </weight>
    </payload>

