<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>737-8.ac</path>
    <main include="737-common.xml" omit-node="y" />

    <model>
        <object-name>Cockpit</object-name>
        <usage>interior</usage>
        <path>Aircraft/737-MAX/Models/Cockpit/cockpit.xml</path>
        <offsets>
            <x-m>2.5765</x-m>
        </offsets>
    </model>

    <!-- Animations -->
    <!-- Landing Gear, Non-retraction -->
    <animation>
        <type>rotate</type>
        <object-name>nlg.steer-pistonL1</object-name>
        <expression>
            <sum>
                <difference>
                    <rad2deg>
                        <atan>
                            <div>
                                <sum>
                                    <value>-0.144</value>
                                    <product>
                                        <value>-0.117316</value>
                                        <sin>
                                            <deg2rad>
                                                <difference>
                                                    <product>
                                                        <value>-156</value>
                                                        <property>gear/gear[0]/steering-norm</property>
                                                    </product>
                                                    <value>34</value>
                                                </difference>
                                            </deg2rad>
                                        </sin>
                                    </product>
                                </sum>
                                <sum>
                                    <value>-0.2278</value>
                                    <product>
                                        <value>-0.117316</value>
                                        <cos>
                                            <deg2rad>
                                                <difference>
                                                    <product>
                                                        <value>-156</value>
                                                        <property>gear/gear[0]/steering-norm</property>
                                                    </product>
                                                    <value>34</value>
                                                </difference>
                                            </deg2rad>
                                        </cos>
                                    </product>
                                </sum>
                            </div>
                        </atan>
                    </rad2deg>
                    <value>16</value>
                </difference>
                <product>
                    <value>156</value>
                    <property>gear/gear[0]/steering-norm</property>
                </product>
            </sum>
        </expression>
        <center>
            <x-m>-13.9412</x-m>
            <y-m>0.096986</y-m>
            <z-m>-0.33289</z-m>
        </center>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.steer-pistonL2</object-name>
        <expression>
            <product>
                <difference>
                    <rad2deg>
                        <atan>
                            <div>
                                <sum>
                                    <value>-0.144</value>
                                    <product>
                                        <value>-0.117316</value>
                                        <sin>
                                            <deg2rad>
                                                <difference>
                                                    <product>
                                                        <value>156</value>
                                                        <property>gear/gear[0]/steering-norm</property>
                                                    </product>
                                                    <value>34</value>
                                                </difference>
                                            </deg2rad>
                                        </sin>
                                    </product>
                                </sum>
                                <sum>
                                    <value>-0.2278</value>
                                    <product>
                                        <value>-0.117316</value>
                                        <cos>
                                            <deg2rad>
                                                <difference>
                                                    <product>
                                                        <value>156</value>
                                                        <property>gear/gear[0]/steering-norm</property>
                                                    </product>
                                                    <value>34</value>
                                                </difference>
                                            </deg2rad>
                                        </cos>
                                    </product>
                                </sum>
                            </div>
                        </atan>
                    </rad2deg>
                    <value>13.55963</value>
                </difference>
                <value>1</value>
            </product>
        </expression>
        <center>
            <x-m>-14.103</x-m>
            <y-m>0.144</y-m>
            <z-m>-0.332649</z-m>
        </center>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.armL</object-name>
        <object-name>nlg.axle</object-name>
        <object-name>nlg.scissorL</object-name>
        <object-name>nlg.scissorU</object-name>
        <object-name>nlg.steer</object-name>
        <object-name>nlg.steer-pistonL1</object-name>
        <object-name>nlg.steer-pistonR1</object-name>
        <object-name>nlg.tire</object-name>
        <property>gear/gear[0]/steering-norm</property>
        <factor>-156</factor>
        <center>
            <x-m>-13.8752</x-m>
            <y-m>0</y-m>
            <z-m>-0.620428</z-m>
        </center>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>nlg.scissorL</object-name>
        <object-name>nlg.tire</object-name>
        <object-name>nlg.axle</object-name>
        <object-name>nlg.armL</object-name>
        <property>gear/gear[0]/compression-m</property>
        <axis>
            <z>1</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.scissorL</object-name>
        <expression>
            <product>
                <difference>
                    <value>135</value>
                    <rad2deg>
                        <acos>
                            <div>
                                <difference>
                                    <value>0.297214693282</value>
                                    <sqr>
                                        <difference>
                                            <value>0.701237</value>
                                            <product>
                                                <property>gear/gear[0]/compression-m</property>
                                                <value>1.000235392</value>
                                            </product>
                                        </difference>
                                    </sqr>
                                </difference>
                                <value>0.297213970782</value>
                            </div>
                        </acos>
                    </rad2deg>
                </difference>
                <value>0.5</value>
            </product>
        </expression>
        <center>
            <x-m>-13.7285</x-m>
            <y-m>0</y-m>
            <z-m>-1.04489</z-m>
        </center>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.scissorU</object-name>
        <expression>
            <product>
                <difference>
                    <value>135</value>
                    <rad2deg>
                        <acos>
                            <div>
                                <difference>
                                    <value>0.297214693282</value>
                                    <sqr>
                                        <difference>
                                            <value>0.701237</value>
                                            <product>
                                                <property>gear/gear[0]/compression-m</property>
                                                <value>1.000235392</value>
                                            </product>
                                        </difference>
                                    </sqr>
                                </difference>
                                <value>0.297213970782</value>
                            </div>
                        </acos>
                    </rad2deg>
                </difference>
                <value>-0.5</value>
            </product>
        </expression>
        <center>
            <x-m>-13.7439</x-m>
            <y-m>0</y-m>
            <z-m>-0.331188</z-m>
        </center>
        <axis>
            <y>1</y>
        </axis>
    </animation>

    <!-- Landing Gear, Retraction -->
    <animation>
        <type>rotate</type>
        <object-name>nlg.doorL</object-name>
        <property>gear/gear[0]/position-norm</property>
        <factor>-90</factor>
        <center>
            <x-m>-14.6459</x-m>
            <y-m>-0.397985</y-m>
            <z-m>0.347816</z-m>
        </center>
        <axis>
            <x>1.67614</x>
            <y>0.000051</y>
            <z>-0.249488</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.doorR</object-name>
        <property>gear/gear[0]/position-norm</property>
        <factor>90</factor>
        <center>
            <x-m>-14.6459</x-m>
            <y-m>0.397985</y-m>
            <z-m>0.347816</z-m>
        </center>
        <axis>
            <x>1.67614</x>
            <y>0.000051</y>
            <z>-0.249488</z>
        </axis>
    </animation>

</PropertyList>