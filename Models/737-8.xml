    </animation>
    <animation>
        <type>rotate</type>
        <object-name>nlg.doorR</object-name>
        <property>gear/gear[0]/position-norm</property>
        <factor>90</factor>
        <center>
            <x-m>-14.6459</x-m>
            <y-m>0.397985</y-m>
            <z-m>0.347816</z-m>
        </center>
        <axis>
            <x>1.67614</x>
            <y>0.000051</y>
            <z>-0.249488</z>
        </axis>
    </animation>

