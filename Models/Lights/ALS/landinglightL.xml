<?xml version="1.0"?>
<PropertyList>

    <path>landinglight.ac</path>
    <nopreview />


    <effect>
        <inherits-from>Aircraft/737-family/Models/lights/ALS/landinglightL</inherits-from>
        <object-name>landinglight</object-name>
    </effect>


    <animation>
        <type>scale</type>
        <x-offset>5.15</x-offset>
        <y-offset>5.15</y-offset>
        <z-offset>5.15</z-offset>
    </animation>


    <animation>
        <type>dist-scale</type>
        <object-name>landinglight</object-name>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.5</dep>
            </entry>
            <entry>
                <ind>300</ind>
                <dep>2</dep>
            </entry>
            <entry>
                <ind>1500</ind>
                <dep>4</dep>
            </entry>
        </interpolation>
    </animation>

    <animation>
        <type>flash</type>
        <object-name>landinglight</object-name>
        <center>


            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>-1</x>


        </axis>
        <power>8</power>
        <factor>6</factor>
        <offset>0</offset>
        <two-sides type="bool">false</two-sides>
        <min>0.5</min>
        <max>1.5</max>
    </animation>


</PropertyList>