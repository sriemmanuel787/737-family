<!-- 737 MAX GabrielYV -->

<PropertyList>

    <!-- Baggage (Front)-->

    <model>
        <name>Baggage</name>
        <path>Aircraft/737-family/Models/Services/Baggage/baggage.xml</path>
        <condition>
            <and>
                <property>controls/switches/baggage1</property>
                <less-than>
                    <property>velocities/airspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>


        </offsets>
    </model>

    <!-- Baggage (Front)-->

    <model>
        <name>baggage</name>
        <path>Aircraft/737-family/Models/Services/Baggage/baggage2.xml</path>
        <condition>
            <and>
                <property>controls/switches/baggage2</property>
                <less-than>
                    <property>velocities/airspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>


        </offsets>
    </model>

    <!-- Fuel truck -->

    <model>
        <name>Fuel truck</name>
        <path>Aircraft/737-family/Models/Services/Fuel/fuel.xml</path>
        <condition>
            <and>
                <property>controls/switches/Fuel</property>
                <less-than>
                    <property>velocities/airspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>


        </offsets>
    </model>


    <!-- Catering Vehicle Front -->

    <model>
        <name>Cateringback Vehicle</name>
        <path>Aircraft/737-family/Models/Services/Catering/catering.xml</path>
        <condition>
            <equals>
                <property>controls/switches/catering1</property>
                <value type="bool">true</value>
            </equals>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>2</value>
            </less-than>
        </condition>
        <offsets>
            <x-m>-10.5</x-m>


        </offsets>
    </model>

    <!-- Catering Vehicle Rear -->

    <model>
        <name>Cateringback Vehicle</name>
        <path>Aircraft/737-family/Models/Services/Catering/catering2.xml</path>
        <condition>
            <equals>
                <property>controls/switches/catering2</property>
                <value type="bool">true</value>
            </equals>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>2</value>
            </less-than>
        </condition>
        <offsets>


        </offsets>
    </model>

    <!-- Bus -->

    <model>
        <name>Bus</name>
        <path>Aircraft/737-family/Models/Services/Bus/bus.xml</path>
        <condition>
            <and>
                <property>controls/switches/bus</property>
                <less-than>
                    <property>velocities/airspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>

            <y-m>-10</y-m>

        </offsets>
    </model>

    <!-- Passenger Stairway (Front)-->

    <model>
        <name>Cone</name>
        <path>Aircraft/737-family/Models/Services/Stairways/stairs.xml</path>
        <condition>
            <and>
                <property>controls/switches/stairs1</property>
                <less-than>
                    <property>velocities/groundspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>
            <x-m>-11</x-m>


        </offsets>
    </model>

    <!-- Passenger Stairway (Rear)-->

    <model>
        <name>Cone</name>
        <path>Aircraft/737-family/Models/Services/Stairways/stairs2.xml</path>
        <condition>
            <and>
                <property>controls/switches/stairs2</property>
                <less-than>
                    <property>velocities/groundspeed-kt</property>
                    <value>2</value>
                </less-than>
            </and>
        </condition>
        <offsets>
            <x-m>-5.5</x-m>


        </offsets>
    </model>

</PropertyList>