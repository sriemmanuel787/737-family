<?xml version="1.0"?>

<PropertyList>

    <sim>
        <minimum-fg-version>2020.3.14</minimum-fg-version>
        <author>Original by Gabriel Hernandez (YV3399), Joshua Davidson (Octal450), legoboyvdlp,
            RenanMsV (BR-RVD), adapted by SP-NTX, Captain Jake, Marsdolphin, Israel Emmanuel
            (naviat)</author>
        <flight-model>jsb</flight-model>
        <gui n="0" include="gui/it-gui.xml" />

        <systems>
            <!-- Disable the generic electrical system -->
            <electrical>
                <path></path>
            </electrical>
            <autopilot n="0">
                <path>Aircraft/737-MAX/Systems/737_pfd.xml</path>
            </autopilot>
            <autopilot n="1">
                <path>Aircraft/737-MAX/Systems/737_fmc_fg.xml</path>
            </autopilot>
            <autopilot n="2">
                <path>Aircraft/737-MAX/Systems/hydraulics.xml</path>
            </autopilot>
            <autopilot n="3">
                <path>Aircraft/737-MAX/Systems/aircraft-systems-logic.xml</path>
            </autopilot>
            <autopilot n="9">
                <path>Aircraft/737-MAX/Systems/autospeedbrake.xml</path>
            </autopilot>
            <autopilot n="10">
                <path>Aircraft/737-MAX/Systems/fl2070_sound.xml</path>
            </autopilot>
            <autopilot n="11">
                <path>Aircraft/737-MAX/Systems/fadec.xml</path>
            </autopilot>
            <autopilot n="12">
                <path>Aircraft/737-MAX/Systems/zoom-views.xml</path>
            </autopilot>
            <path>Aircraft/737-MAX/Systems/systems.xml</path>
            <property-rule n="101">
                <path>Aircraft/737-MAX/Systems/instrument-filters.xml</path>
            </property-rule>
            <wingflexer include="Systems/wingflexer-params.xml" />
            <autopilot n="13">
                <name>wing flexer property rule</name>
                <path>Aircraft/Generic/wingflexer.xml</path>
            </autopilot>
            <autopilot>
                <name>IT-AUTOFLIGHT</name>
                <path>Systems/it-autoflight.xml</path>
            </autopilot>
            <autopilot>
                <name>IT-AUTOTHRUST</name>
                <path>Systems/it-autothrust.xml</path>
            </autopilot>
            <autopilot>
                <name>Gear AGL FT</name>
                <path>Systems/gear-agl-ft.xml</path>
            </autopilot>
        </systems>

        <instrumentation>
            <path>Aircraft/737-MAX/Systems/instrumentation.xml</path>
        </instrumentation>

        <sound>
            <path>Aircraft/737-MAX/Sounds/737-sound.xml</path>
        </sound>

        <virtual-cockpit archive="y">true</virtual-cockpit>
        <allow-toggle-cockpit archive="y">false</allow-toggle-cockpit>

        <model>
            <pushback>
                <kp type="double">1000</kp>
                <ki type="double">25</ki>
                <kd type="double">0</kd>
                <position-norm type="double">0</position-norm>
            </pushback>
            <autopush include="Models/autopush-config.xml" />
            <icing>
                <iceable>
                    <name>Wing</name>
                    <ice-inches>0.0</ice-inches>
                    <salvage-control>/controls/deice/wing</salvage-control>
                    <sensitivity type="double">0.8</sensitivity>
                    <output-property>/fdm/jsbsim/ice/wing</output-property>
                </iceable>
                <iceable>
                    <name>LeftEngine</name>
                    <ice-inches>0.0</ice-inches>
                    <salvage-control>/controls/deice/lengine</salvage-control>
                    <sensitivity type="double">0.5</sensitivity>
                    <output-property>/fdm/jsbsim/ice/lengine</output-property>
                </iceable>
                <iceable>
                    <name>RightEngine</name>
                    <ice-inches>0.0</ice-inches>
                    <salvage-control>/controls/deice/rengine</salvage-control>
                    <sensitivity type="double">0.5</sensitivity>
                    <output-property>/fdm/jsbsim/ice/rengine</output-property>
                </iceable>
                <iceable>
                    <name>Windshield</name>
                    <ice-inches>0.0</ice-inches>
                    <salvage-control>/controls/deice/windowprobeheat</salvage-control>
                    <sensitivity type="double">0.5</sensitivity>
                    <!-- not relevant for fdm, ignore output-property -->
                </iceable>
                <iceable>
                    <name>Fuselage</name>
                    <ice-inches>0.0</ice-inches>
                    <!-- no salvage control, no deicing of the fuselage -->
                    <sensitivity type="double">0.3</sensitivity>
                    <output-property>/fdm/jsbsim/ice/fuselage</output-property>
                </iceable>
                <iceable>
                    <name>Pitot tube</name>
                    <ice-inches>0.0</ice-inches>
                    <sensitivity type="double">1</sensitivity>
                    <salvage-control>/controls/deice/windowprobeheat</salvage-control>
                    <output-property>/systems/pitot/icing</output-property>
                </iceable>
            </icing>
        </model>

        <view n="1">
            <config>
                <z-offset-m type="double" archive="y">-80.0</z-offset-m>
            </config>
        </view>

        <chase-distance-m>-60</chase-distance-m>

        <multiplay>
            <generic>
                <int n="0" alias="/systems/electrical/outputs/strobe" />
                <int n="1" alias="/systems/electrical/outputs/beacon" />
                <int n="2" alias="/controls/lighting/taxi-light" />
                <int n="3" alias="/controls/lighting/logo-lights" />
                <int n="4" alias="/controls/lighting/landing-lights" />
                <int n="5" alias="/controls/lighting/nav-lights" />
                <int n="6" alias="/gear/gear[0]/tyre-smoke" />
                <int n="7" alias="/gear/gear[1]/tyre-smoke" />
                <int n="8" alias="/gear/gear[2]/tyre-smoke" />
                <float n="0" alias="/gear/gear[0]/compression-m" />
                <float n="1" alias="/gear/gear[1]/compression-m" />
                <float n="2" alias="/gear/gear[2]/compression-m" />
                <float n="3" alias="/gear/gear[1]/compression-m" />
                <float n="4" alias="/gear/gear[2]/compression-m" />
                <float n="5" alias="/surface-positions/speedbrake-norm" />
                <float n="6" alias="/surface-positions/reverser-norm" />
                <float n="7" alias="/controls/flight/aileron-left" />
                <float n="8" alias="/controls/flight/aileron-right" />
                <float n="9" alias="/controls/flight/elevator-left" />
                <float n="10" alias="/controls/flight/elevator-right" />
                <float n="11" alias="/controls/flight/rudder-sum" />
                <string alias="/sim/multiplay/callsign" />
            </generic>
        </multiplay>

        <flight-recorder include="Systems/flight-recorder.xml" />

        <submodels>
            <serviceable>true</serviceable>
            <path>submodels.xml</path>
        </submodels>

        <menubar>
            <default>
                <menu n="100">
                    <label>737 MAX</label>
                    <enabled type="bool">true</enabled>
                    <item>
                        <label>Aircraft Config</label>
                        <binding>
                            <command>dialog-show</command>
							<dialog-name>acconfig-main</dialog-name>
                        </binding>
                    </item>
                    <item>
                        <label>Select Livery</label>
                        <binding>
                            <command>nasal</command>
                            <script>aircraft.livery.dialog.toggle();</script>
                        </binding>
                    </item>
                    <item n="10">
                        <label>Pushback</label>
                        <name>autopush</name>
                        <binding>
                            <command>dialog-show</command>
                            <dialog-name>autopush</dialog-name>
                        </binding>
                    </item>
                </menu>
            </default>
        </menubar>

        <checklists include="737-MAX-checklist.xml" />
    </sim>

    <it-autoflight>
        <config>
            <roll>
                <kp-low>0.11</kp-low> <!-- Kp at low speed -->
                <kp-high>0.05</kp-high> <!-- Kp at high speed -->
                <ti>1.0</ti>
                <td>0.0001</td>
            </roll>
            <pitch>
                <kp-low>-0.14</kp-low> <!-- Kp at low speed -->
                <kp-high>-0.06</kp-high> <!-- Kp at high speed -->
                <ti>0.5</ti>
                <td>0.001</td>
            </pitch>
            <cmd>
                <alt>-5</alt>
                <flch-accel>1.5</flch-accel>
                <roll>1.6</roll>
            </cmd>
            <autoland>
                <pitch-kp>0.0051</pitch-kp>
                <yaw-kp>-0.05</yaw-kp>
            </autoland>
            <rollout>
                <roll-kp>-0.1</roll-kp>
                <pitch-nose>0.1</pitch-nose>
                <pitch-rate>-1.5</pitch-rate>
                <yaw-kp>-0.02</yaw-kp>
            </rollout>
        </config>
        <settings>
            <accel-ft type="double">1000</accel-ft> <!-- Acceleration AGL when T/O CLB changes to SPD
            CLB, 0 to disable -->
            <auto-bank-limit-calc type="bool">1</auto-bank-limit-calc> <!-- Disable to add a custom
            auto bank limit schedule -->
            <autoland-without-ap type="bool">1</autoland-without-ap> <!-- Engage LAND and FLARE Modes
            even if the AP is off -->
            <autothrottle-max type="double">0.95</autothrottle-max> <!-- Thrust Max Limit Normalized -->
            <autothrottle-min type="double">0.02</autothrottle-min> <!-- Thrust Low Limit Normalized -->
            <bank-max-deg type="double">25</bank-max-deg> <!-- Maximum Bank Limit -->
            <custom-fma type="bool">0</custom-fma> <!-- Call functions when modes change for a custom
            FMA to be implemented -->
            <disable-final type="bool">0</disable-final> <!-- Disable the Final Controllers, for
            custom FCS integration -->
            <fd-starts-on type="bool">0</fd-starts-on> <!-- Enable/Disable Flight Director being on
            by default -->
            <fd-takeoff-offset type="double">0</fd-takeoff-offset> <!-- Adjust the Flight Director
            Pitch Bar on ground -->
            <hdg-hld-separate type="bool">0</hdg-hld-separate> <!-- Separates the HDG HLD mode from
            the HDG SEL mode -->
            <land-enable type="bool">1</land-enable> <!-- Enable/Disable Autoland -->
            <land-flap type="double">0.7</land-flap> <!-- Minimum Flap used for Landing -->
            <lnav-ft type="double">50</lnav-ft> <!-- AGL when LNAV becomes active if armed -->
            <retard-enable type="bool">1</retard-enable> <!-- AGL to Thrust Retard -->
            <retard-ft type="double">50</retard-ft> <!-- Enable Thrust Retard -->
            <takeoff-hdg-cap type="double">5</takeoff-hdg-cap> <!-- Maximum bank to capture current
            hdg in T/O mode -->
            <toga-spd type="double">160</toga-spd> <!-- V2 + 10kts -->
            <use-controls-engines type="bool">1</use-controls-engines> <!-- Use /controls/engines
            properties -->
            <use-controls-flight type="bool">1</use-controls-flight> <!-- Use /controls/flight
            properties -->
        </settings>
    </it-autoflight>

    <lighting>
        <interior type="double">0</interior>
        <panel-aft-norm type="double">0</panel-aft-norm>
        <panel-aft-flood type="double">0</panel-aft-flood>
        <panel-fwd-norm type="double">0</panel-fwd-norm>
        <panel-afds-norm type="double">0</panel-afds-norm>
        <mfd-captL type="double">1</mfd-captL>
        <mfd-captR type="double">1</mfd-captR>
        <mfd-foL type="double">1</mfd-foL>
        <mfd-foR type="double">1</mfd-foR>
        <cockpit type="bool">false</cockpit>
        <dome-norm type="double">0</dome-norm>
        <overhead-panel-norm type="double">0</overhead-panel-norm>
    </lighting>

    <consumables>
        <fuel>
            <tank n="0">
                <name>Left Main Tank</name>
                <level-lbs archive="y">6165</level-lbs>
            </tank>
            <tank n="1">
                <name>Right Main Tank</name>
                <level-lbs archive="y">6165</level-lbs>
            </tank>
            <tank n="2">
                <name>Center Tank</name>
                <level-lbs archive="y">0</level-lbs>
            </tank>
            <tank n="3">
                <hidden type="bool">1</hidden>
            </tank>
            <tank n="4">
                <hidden type="bool">1</hidden>
            </tank>
        </fuel>
    </consumables>

    <engines>
        <engine n="0">
            <egt-actual type="double">0</egt-actual>
            <state type="int">0</state>
            <running>false</running>
            <reverser-pos-norm type="double">0</reverser-pos-norm>
        </engine>
        <engine n="1">
            <egt-actual type="double">0</egt-actual>
            <state type="int">0</state>
            <running>false</running>
            <reverser-pos-norm type="double">0</reverser-pos-norm>
        </engine>
    </engines>

    <!-- Custom Keybindings -->
    <input>
        <keyboard>
            <key n="71">
                <name>G</name>
                <desc>Gear Down Custom Binding</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/b737/controls/gear/lever</property>
                    <value>0</value>
                </binding>
            </key>

            <key n="103">
                <name>g</name>
                <desc>Gear Up Custom Binding</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/b737/controls/gear/lever</property>
                    <value>1</value>
                </binding>
            </key>
            <key n="106">
                <name>j</name>
                <desc>Decrease spoiler lever position</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>b737/controls/flight/spoilers-lever-pos</property>
                    <step type="double">-1</step>
                    <min type="double">0</min>
                    <max type="double">6</max>
                </binding>
            </key>

            <key n="107">
                <name>k</name>
                <desc>Increase spoiler lever position</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>b737/controls/flight/spoilers-lever-pos</property>
                    <step type="double">1</step>
                    <min type="double">0</min>
                    <max type="double">6</max>
                </binding>
            </key>

            <key n="2">
                <name>Ctrl-b</name>
                <desc>Cycle speedbrake setting</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/flight/autospeedbrake</property>
                    <value>0</value>
                </binding>
                <binding>
                    <command>property-cycle</command>
                    <property>/b737/controls/flight/spoilers-lever-pos</property>
                    <value>0</value>
                    <value>6</value>
                </binding>
            </key>

            <key n="108">
                <name>l</name>
                <desc>Toggle landing lights</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/lighting/landing-lights</property>
                </binding>
            </key>


            <key n="76">
                <name>L</name>
                <desc>Toggle taxi lights</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/lighting/Taxi-light</property>
                </binding>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/lighting/RB-light</property>
                </binding>
            </key>

            <key n="82">
                <name>R</name>
                <desc>Decrease auto-brake setting</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>controls/gear/autobrakes</property>
                    <step>-1</step>
                    <min>-1</min>
                    <max>5</max>
                    <wrap>true</wrap>
                </binding>
            </key>

            <key n="114">
                <name>r</name>
                <desc>Increase auto-brake setting</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>controls/gear/autobrakes</property>
                    <step>1</step>
                    <min>-1</min>
                    <max>5</max>
                    <wrap>true</wrap>
                </binding>
            </key>


            <key n="127">
                <name>Delete</name>
                <desc>Thrust reversers toggle</desc>
                <binding>
                    <condition>
                        <and>
                            <property>/sim/input/selected/engine[0]</property>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[0]</property>
                                <value>0.03</value>
                            </less-than>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[1]</property>
                                <value>0.03</value>
                            </less-than>
                            <or>
                                <less-than>
                                    <property>/instrumentation/radar-altimeter[0]/radar-altitude-ft</property>
                                    <value>17.5</value>
                                </less-than>
                                <property>/b737/sensors/air-ground</property>
                            </or>
                            <equals>
                                <property>/engines/engine[0]/reverser-pos-norm</property>
                                <value>0</value>
                            </equals>
                        </and>
                    </condition>
                    <command>property-interpolate</command>
                    <property>/engines/engine[0]/reverser-pos-norm</property>
                    <value>1</value>
                    <time>2</time>
                </binding>
                <binding>
                    <condition>
                        <and>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[0]</property>
                                <value>0.03</value>
                            </less-than>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[1]</property>
                                <value>0.03</value>
                            </less-than>
                            <equals>
                                <property>/engines/engine[0]/reverser-pos-norm</property>
                                <value>1</value>
                            </equals>
                        </and>
                    </condition>
                    <command>property-interpolate</command>
                    <property>/engines/engine[0]/reverser-pos-norm</property>
                    <value>0</value>
                    <time>3</time>
                </binding>
                <binding>
                    <condition>
                        <and>
                            <property>/sim/input/selected/engine[1]</property>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[0]</property>
                                <value>0.03</value>
                            </less-than>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[1]</property>
                                <value>0.03</value>
                            </less-than>
                            <or>
                                <less-than>
                                    <property>/instrumentation/radar-altimeter[0]/radar-altitude-ft</property>
                                    <value>17.5</value>
                                </less-than>
                                <property>/b737/sensors/air-ground</property>
                            </or>
                            <equals>
                                <property>/engines/engine[1]/reverser-pos-norm</property>
                                <value>0</value>
                            </equals>
                        </and>
                    </condition>
                    <command>property-interpolate</command>
                    <property>/engines/engine[1]/reverser-pos-norm</property>
                    <value>1</value>
                    <time>2</time>
                </binding>
                <binding>
                    <condition>
                        <and>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[0]</property>
                                <value>0.03</value>
                            </less-than>
                            <less-than>
                                <property>/autopilot/internal/servo-throttle[1]</property>
                                <value>0.03</value>
                            </less-than>
                            <equals>
                                <property>/engines/engine[1]/reverser-pos-norm</property>
                                <value>1</value>
                            </equals>
                        </and>
                    </condition>
                    <command>property-interpolate</command>
                    <property>/engines/engine[1]/reverser-pos-norm</property>
                    <value>0</value>
                    <time>3</time>
                </binding>
            </key>

            <key n="109">
                <name>m</name>
                <desc>Set full throttle</desc>
                <binding>
                    <command>nasal</command>
                    <script>boeing737.setMaxPower();</script>
                </binding>
            </key>

            <key n="110">
                <name>n</name>
                <desc>Set idle throttle</desc>
                <binding>
                    <command>nasal</command>
                    <script>boeing737.setIdlePower();</script>
                </binding>
            </key>
        </keyboard>
    </input>

    <switches>
        <increase-fps type="bool">0</increase-fps>
    </switches>

    <autopilot>
        <display>
            <roll-mode type="string"></roll-mode>
            <pitch-mode type="string"></pitch-mode>
            <throttle-mode type="string"></throttle-mode>
            <roll-mode-armed type="string"></roll-mode-armed>
            <pitch-mode-armed type="string"></pitch-mode-armed>
            <throttle-mode-armed type="string"></throttle-mode-armed>
            <roll-mode-last-change type="double">0</roll-mode-last-change>
            <pitch-mode-last-change type="double">0</pitch-mode-last-change>
            <throttle-mode-last-change type="double">0</throttle-mode-last-change>
            <roll-mode-rectangle type="bool">0</roll-mode-rectangle>
            <pitch-mode-rectangle type="bool">0</pitch-mode-rectangle>
            <throttle-mode-rectangle type="bool">0</throttle-mode-rectangle>
            <localizer-expanded type="bool">0</localizer-expanded>
            <afds-mode type="string" n="0"></afds-mode>
            <afds-mode type="string" n="1"></afds-mode>
            <afds-mode-rectangle type="bool" n="0">0</afds-mode-rectangle>
            <afds-mode-rectangle type="bool" n="1">0</afds-mode-rectangle>
            <afds-mode-last-change type="double" n="0">0</afds-mode-last-change>
            <afds-mode-last-change type="double" n="1">0</afds-mode-last-change>
        </display>
    </autopilot>

    <controls>
        <bleed>
            <apu-on type="bool">0</apu-on>
            <isolation-valve type="bool">0</isolation-valve>
            <l-recirc-fan type="int">0</l-recirc-fan>
            <n1-on type="bool">0</n1-on>
            <n2-on type="bool">0</n2-on>
            <ovht-test type="bool">0</ovht-test>
            <pack-l type="int">0</pack-l>
            <pack-r type="int">0</pack-r>
            <r-recirc-fan type="int">0</r-recirc-fan>
            <trip-reset type="bool">0</trip-reset>
        </bleed>
        <elec>
            <ac-mode type="int">0</ac-mode>
            <apu-gen-1 type="bool">0</apu-gen-1>
            <apu-gen-2 type="bool">0</apu-gen-2>
            <bus-trans type="bool">0</bus-trans>
            <cabin type="bool">0</cabin>
            <dc-mode type="int">0</dc-mode>
            <dc-on type="int">0</dc-on>
            <drive-1-conn type="bool">0</drive-1-conn>
            <drive-2-conn type="bool">0</drive-2-conn>
            <gen-1 type="bool">0</gen-1>
            <gen-2 type="bool">0</gen-2>
            <grd-power type="bool">0</grd-power>
            <ife type="bool">0</ife>
            <standby-pwr type="int">0</standby-pwr>
        </elec>
        <eng>
            <apu type="int">0</apu>
            <eng-mode type="int">0</eng-mode>
            <n1-start type="int">0</n1-start>
            <n2-start type="int">0</n2-start>
        </eng>
        <flt-control>
            <alt-flaps-arm type="bool">0</alt-flaps-arm>
            <alt-flaps type="int">0</alt-flaps>
            <rudder-a type="int">0</rudder-a>
            <rudder-b type="int">0</rudder-b>
            <spoiler-a type="bool">0</spoiler-a>
            <spoiler-b type="bool">0</spoiler-b>
            <yaw-damper type="bool">0</yaw-damper>
